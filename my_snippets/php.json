{
  "logger": {
    "prefix": "@vlogger",
    "body": [
      "\\viridem\\logger\\ViridemLogger::getLogger(__CLASS__)->info('DANNICK-' . var_dump_return($1));"
    ]
  },

  "bracktrace-logger": {
    "prefix": "@backtrace",
    "body": [
      "\\viridem\\logger\\ViridemLogger::getLogger(__CLASS__)->info('DANNICK-backtrace-' . var_dump_return(debug_backtrace(DEBUG_BACKTRACE_IGNORE_ARGS))); "
    ]
  }
}
