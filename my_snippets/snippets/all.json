{
  "checkbox": {
    "prefix": "-",
    "body": [
      "- [ ] $1"
    ],
    "description": "Create a checkbox"
  },
  "logger": {
    "prefix": "viridem-logger",
    "body": [
      "\\viridem\\logger\\ViridemLogger::getLogger(__FILE__)->info('DANNICK-' . var_dump_return($1));"
    ],
    "description": "Template de base d'un log dans Viridem."
  },
  "sm-logger": {
    "prefix": "@vlog",
    "body": [
      "\\viridem\\logger\\ViridemLogger::getLogger(__FILE__)->info('DANNICK-$2', $1);"
    ],
    "description": "Template de base d'un log dans Viridem."
  },
  "laravel-logger": {
    "prefix": "@llog",
    "body": [
      "\\Illuminate\\Support\\Facades\\Log::debug('Dannick : ' . var_dump_return($1));"
    ],
    "description": "Template de base d'un log dans Viridem."
  },
  "wordpresslogger": {  
    "prefix": "wordpress-logger",
    "body": [
      "error_log(print_r($1 ,true));"
    ],
    "description": "wordpress logger"
  },
  "laravellogger": {
    "prefix": "laravel-logger",
    "body": [
      "\\Illuminate\\Support\\Facades\\Log::debug('Dannick : ' . var_dump_return($1));"
    ],
    "description": "laravel logger"
  },
	"php": {
		"prefix": "php-balise",
		"body": [
			"<?php ",
			"",
			"$1",
			"",
			"?>"
		],
		"description": "php tag"
	},
	"phpVar": {
		"prefix": "@var",
		"body": [
			"/** @var $1 */"
		],
		"description": "php var tag"
	},
	"phpHeaderComment": {
		"prefix": "@comment",
		"body": [
			"/**",
			" * $1",
			" * ",
			" * @param type $variable",
			" * @return void",
			" */"
		],
		"description": "php var tag"
	},
	"phpHeaderCommentParams": {
		"prefix": "@param",
		"body": [
			" @param $2 $1"
		],
		"description": "php param doc tag"
	},
	"phpHeaderCommentReturn": {
		"prefix": "@return",
		"body": [
			" @return $1"
		],
		"description": "php return doc tag"
	}
}
